  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiao1272209235/jiahe-vscode-kit.git"
  },
  "homepage": "https://github.com/xiao1272209235/jiahe-vscode-kit/blob/main/README.md"
}
